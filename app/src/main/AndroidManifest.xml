<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sord.captureu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tree"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tree"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:name=".Activities.GG"
        >
        <activity android:name=".Activities.CreatePassword"></activity>
        <activity android:name=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.Login"></activity>
        <activity android:name=".Activities.Settings"
            android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.Options"
            android:parentActivityName=".Activities.MainActivity"></activity>
        <activity android:name=".Activities.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>